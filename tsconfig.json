{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "moduleSuffixes": [".ios", ".android", ""],
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/assets/*": ["src/assets/*"],
      "@/navigators/*": ["src/navigators/*"],
      "@/screens/*": ["src/screens/*"],
      "@/libs/*": ["src/libs/*"]
    }
  }
}
